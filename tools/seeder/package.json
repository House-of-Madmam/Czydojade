{
  "name": "seeder",
  "type": "module",
  "scripts": {
    "seed": "npm run seed:stops && npm run seed:lines",
    "seed:stops": "node --env-file=.env src/stops.ts",
    "seed:lines": "node --env-file=.env src/lines.ts",
    "build": "tsc --build",
    "build:dev": "tsc --build",
    "lint": "eslint . -c ../../eslint.config.mjs",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "drizzle-orm": "0.44.3",
    "pg": "8.16.3",
    "uuid": "11.0.3"
  }
}
