{
  "name": "czydojade",
  "scripts": {
    "dev": "concurrently --names=frontend,backend --prefix-colors=blue,green \"npm run dev --workspace=apps/frontend\" \"npm run dev --workspace=apps/backend\"",
    "build": "turbo run build",
    "build:dev": "turbo run build:dev",
    "test": "turbo run test:ci --concurrency 5",
    "lint": "turbo run lint --concurrency 5",
    "lint:fix": "turbo run lint:fix"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "5.4.0",
    "@types/node": "24.6.2",
    "concurrently": "9.2.1",
    "cross-env": "10.1.0",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-perfectionist": "4.15.0",
    "prettier": "3.6.2",
    "turbo": "2.5.8",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0"
  },
  "volta": {
    "node": "24.9.0",
    "npm": "11.6.1"
  },
  "engines": {
    "node": ">=24",
    "npm": ">=11"
  },
  "workspaces": [
    "apps/*"
  ],
  "packageManager": "npm@11.6.1",
  "dependencies": {
    "drizzle-kit": "0.31.5",
    "drizzle-orm": "0.44.6"
  }
}
